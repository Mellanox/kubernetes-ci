kind: Cluster
apiVersion: kind.x-k8s.io/v1alpha4
{% if is_ovn_kubernetes %}
networking:
  disableDefaultCNI: true
  apiServerAddress: {{ ansible_default_ipv4.address }}
featureGates:
  SCTPSupport: true
{% endif %}
nodes:
 - role: control-plane

{%- for _ in range(num_worker | int) %}

 - role: worker
{%- endfor %}
